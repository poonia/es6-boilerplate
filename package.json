{
  "name": "es6-boilerplate",
  "version": "1.0.0",
  "description": "Simple ES6 boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel server lint:watch",
    "server": "webpack-dev-server --hot --inline",
    "watch": "webpack -w -d",
    "build": "webpack -p",
    "lint": "node_modules/.bin/esw webpack.config.* src --color",
    "lint:watch": "npm run lint -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poonia/es6-boilerplate.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/poonia/es6-boilerplate/issues"
  },
  "homepage": "https://github.com/poonia/es6-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^5.16.0",
    "eslint-watch": "^5.1.2",
    "npm-run-all": "^4.0.2",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1"
  }
}
